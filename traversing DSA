traversing in array DSA

#include<stdio.h>
#include<conio.h>
void main()
{
     int a[50], i,size;
     printf("enter size of aarray");
     scanf("%d",&size);
     printf("enter elements of array");
     for(i=0;i<size;i++)
     {
          scanf("%d",&a[i]);
     }
     printf("elements of array");
     for(i=0;i<size;i++)
     {
          printf("%d",a[i]);
          
     }
          getch();
          
}
