#include<stdio.h>
#include<conio.h>
void main()
{
int x[5];
int i,n;
printf("enter the elements of array");
for(i=0;i<5;i++)
{
scanf("%d",&x[i]);
}
printf(" required even no.s");
for(i=0;i<5;i++)
{
if(x[i]%2==0)
{
printf("\t%d",x[i]);
}
}
printf("\n required odd no.");
for(i=0;i<5;i++)
{
if(x[i]%2!=0)
{
printf("\t%d",x[i]);
}
}
getch();
clrscr();
}
